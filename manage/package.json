{
  "private": true,
  "name": "manage",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "yarn generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "yarn generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public -k links",
    "generate:css": "yarn tailwindcss -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production yarn dev:wrangler",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@remix-run/cloudflare": "^1.6.3",
    "@remix-run/cloudflare-pages": "^1.6.3",
    "@remix-run/react": "^1.6.3",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@remix-run/dev": "^1.6.3",
    "@remix-run/eslint-config": "^1.6.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
